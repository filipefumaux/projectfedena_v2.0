<div id="content-header">
  <img src="/images/show_finance.png" alt=""/>

  <h1><%= t('feesCreate') %></h1>

  <h3><%= t('masterFeesandAdditionalFees') %></h3>

  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png", :border => 0), "history.back()" %>
  </div>
</div>
<div id="page-yield">
  <% unless flash[:notice].nil? %>
      <p class="flash-msg"><%= flash[:notice] %></p>
  <% end %>

  <div class="box">

    <div class="link-box">
      <div class="link-heading"><%= link_to t('masterFees'), :action => "master_fees" %></div>
      <div class="link-descr"><%= t('createsMasterFees') %></div>
    </div>

    <div class="link-box">
      <div class="link-heading"><%= link_to t("finance.additionalFees"), :action => "additional_fees" %></div>
      <div class="link-descr"><%= t("creates") %> <%= t("finance.additionalFees") %></div>
    </div>


  </div>
</div>