<div id="content-header">
  <img src="/images/show_finance.png" alt=""/>

  <h1><%= t('feesSubmission') %></h1>

  <h3><%= t('student.feesSubmission') %></h3>

  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png", :border => 0), "history.back()" %>
  </div>
</div>
<div id="page-yield">
  <div class="box">
    <% unless flash[:notice].nil? %>
        <p class="flash-msg"><%= flash[:notice] %></p>
    <% end %>
    <div class="link-box">
      <div class="link-heading"><%= link_to t('finance.feesSubmissionByCourse'), :action => "fees_submission_batch" %> </div>
      <div class="link-descr"><%= t('finance.descFeesSubmissionByCourse') %></div>
    </div>

    <div class="link-box">
      <div class="link-heading"><%= link_to t('finance.feesSubmissionForStudent'), :action => "fees_student_search" %> </div>
      <div class="link-descr"><%= t('finance.descFeesSubmissionForStudent') %></div>
    </div>

  </div>
</div>
